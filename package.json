{
  "name": "mzqc",
  "version": "1.0.18-1",
  "description": "Rapid Front-end Project Engineering Tool",
  "keywords": [
    "project",
    "building"
  ],
  "license": "MIT",
  "author": "muzhongqing",
  "bin": {
    "gc": "./.editorconfig-generator.js",
    "sort": "./.sort-package.js",
    "rmc": "./.remove-comments.js",
    "adds": "./.add-scripts.js"
  },
  "scripts": {
    "gc": "node ./.editorconfig-generator.js",
    "cmt": "node ./cmt.js",
